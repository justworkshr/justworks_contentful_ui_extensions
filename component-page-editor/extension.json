{
  "id": "component-page-editor",
  "name": "component-page-editor",
  "srcdoc": "./build/index.html",
  "parameters": {
    "installation": [
      {
        "id": "productionUrl",
        "type": "Symbol",
        "name": "Production URL",
        "default": "https://justworks.com",
        "required": true
      },
      {
        "id": "stagingUrl",
        "type": "Symbol",
        "name": "Staging URL",
        "default": "https://justworks-staging-v2.herokuapp.com",
        "required": true
      }
    ],
    "instance": [
      {
        "id": "moduleContentType",
        "type": "Symbol",
        "name": "Module Content Type ID",
        "default": "viewComponent",
        "required": true
      },
      {
        "id": "title",
        "type": "Symbol",
        "name": "Extension Title",
        "default": "Component Page Builder",
        "required": false
      }
    ]
  }
}
